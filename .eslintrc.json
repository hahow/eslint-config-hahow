{
    "extends": "standard",
    "parserOptions": {
        "sourceType": "script"
    },
    "rules": {
        "no-var": "error",
        "object-shorthand": "error",
        "prefer-template": "error",
        "strict": ["error", "global"],
        "arrow-parens": "error",
        "prefer-arrow-callback": "error",
        "arrow-body-style": ["error", "as-needed"],
        "no-shadow": "error"
    }
}
